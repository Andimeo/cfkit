{
    "C#": {
        "Mono": "mcs -o+ -out:{output}.exe {file} && mono {output}.exe < {input_file} > {output_file}"
    },
    "Go": "go run {file} < {input_file} > {output_file}",
    "Haskell GHC": "runghc {file} < {input_file} > {output_file}",   
    "Java": "java -Duser.language=en -Duser.region=US -Duser.variant=US {file} < {input_file} > {output_file}",
    "Kotlin": "kotlinc {file} -include-runtime -d {output}.jar && java -jar {output}.jar < {input_file} > {output_file}",
    "Scala": "scala {file} < {input_file} > {output_file}"
}