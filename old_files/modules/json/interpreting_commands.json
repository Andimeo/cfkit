{
    "PHP": "php -n -d display_errors=On -d error_reporting=E_ALL {file} < {input_file} > {output_file}",
    "Python": "{command} < {input_file} > {output_file}",
    "Perl": "perl {file} < {input_file} > {output_file}",  
    "Ruby": "ruby {file} < {input_file} > {output_file}",
    "JavaScript": {
        "JavaScript d8": "d8 {file} < {input_file} > {output_file}",
        "Node.js": "node {file} < {input_file} > {output_file}}"
    }
}