{
    "C": {
        "win32": "gcc.exe -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -lm -s -Wl,--stack={stack_size} -O2 -o {output}.exe {file}",
        "linux": "ulimit -t {time_limit} -s {stack_size} && gcc -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -lm -s -O2 -o {output}.out {file}",
        "darwin": "ulimit -t {time_limit} -s {stack_size} && gcc -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -lm -s -O2 -o {output}.out {file}"
    },
    "C++": {
        "Clang++": {
            "Clang++17": {
                "win32": "clang++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++17 -Wl,-stack_size,{stack_size} -o {output}.exe {file}",
                "linux": "ulimit -t {time_limit} -s {stack_size} && clang++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++17 -o {output}.out {file}",
                "darwin": "ulimit -t {time_limit} -s {stack_size} && g++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++20 -o {output}.out {file}"
            },
            "Clang++20": {
                "win32": "clang++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++20 -Wl,-stack_size,{stack_size} -o {output}.exe {file}",
                "linux": "ulimit -t {time_limit} -s {stack_size} && clang++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++20 -o {output}.out {file}",
                "darwin": "ulimit -t {time_limit} -s {stack_size} && g++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++20 -o {output}.out {file}"
            }
        },
        "GNU G++": {
            "GNU G++ 14": {
                "win32": "g++.exe -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -Wl,--stack,{stack_size} -O2 -std=c++14 -o {output}.exe {file}",
                "linux": "ulimit -t {time_limit} -s {stack_size} && g++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++14 -o {output}.out {file}",
                "darwin": "ulimit -t {time_limit} -s {stack_size} && g++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++20 -o {output}.out {file}"
            },
            "GNU G++ 17": {
                "win32": "g++.exe -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -Wl,--stack={stack_size} -O2 -std=c++17 -o {output}.exe {file}",
                "linux": "ulimit -t {time_limit} -s {stack_size} && g++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++17 -o {output}.out {file}",
                "darwin": "ulimit -t {time_limit} -s {stack_size} && g++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++20 -o {output}.out {file}"
            },
            "GNU G++ 20": {
                "win32": "g++.exe -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -Wl,--stack={stack_size} -O2 -std=c++20 -o {output}.exe {file}",
                "linux": "ulimit -t {time_limit} -s {stack_size} && g++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++20 -o {output}.out {file}",
                "darwin": "ulimit -t {time_limit} -s {stack_size} && g++ -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -std=c++20 -o {output}.out {file}"
            }
        },
        "Microsoft Visual Studio C++ 2017": {
            "win32": "cl /std:c++17 /W4 /F{stack_size} /EHsc /O2 /Fe:{output}.exe {file}",
            "linux": null,
            "darwin": null
        }
    },
    "C#": {
        ".NET": {
            "win32": "csc.exe /o+ /out:{output}.exe {file}",
            "linux": null,
            "darwin": null
        }
    },
    "D": {
        "DMD": {
            "win32": "dmd -L/STACK:{stack_size} -O -release {file} -of={output}.exe",
            "linux": "ulimit -t {time_limit} -s {stack_size} && dmd -O -release {file} -of={output}.out",
            "darwin": "ulimit -t {time_limit} -s {stack_size} && dmd -O -release {file} -of={output}.out"
        },
        "GDC": {
            "win32": "gdc -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -o {output}.exe {file}",
            "linux": "ulimit -t {time_limit} -s {stack_size} && gdc -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -o {output}.out {file}",
            "darwin": "ulimit -t {time_limit} -s {stack_size} && gdc -Wall -Wextra -Wconversion -fno-strict-aliasing -pedantic -Wshadow -Wfloat-equal -Wlogical-op -O2 -o {output}.out {file}"
        }
    },
    "OCaml ": {
        "win32": "ocamlopt nums.cmxa str.cmxa -pp camlp4o -o {output}.exe -ocaml {file}",
        "linux": "ulimit -t {time_limit} -s {stack_size} && ocamlopt nums.cmxa str.cmxa -pp camlp4o -o {output}.exe -ocaml {file}",
        "darwin": "ulimit -t {time_limit} -s {stack_size} && ocamlopt nums.cmxa str.cmxa -pp camlp4o -o {output}.exe -ocaml {file}"
    },
    "Delphi 7": {
        "win32": "dcc{architecture} -Q -cc {file} && ren {file}.exe",
        "linux": null,
        "darwin": null
    },
    "Pascal": {
        "Free Pascal": {
            "win32": "fpc -n -O2 -viwn -Mdelphi {file} -o {output}.exe",
            "linux": null,
            "darwin": null
        },
        "PascalABC.NET": {
            "win32": "pabcnetc {file} && ren {file}.exe {output}.exe && ren {file}.dpr {output}.dpr",
            "linux": null,
            "darwin": null
        }
    },
    "Rust": {
        "win32": "rustc -C opt-level=2 -o {output}.exe {file}",
        "linux": "ulimit -t {time_limit} -s {stack_size} && rustc -C opt-level=2 -o {output}.exe {file}",
        "darwin": "ulimit -t {time_limit} -s {stack_size} && rustc -C opt-level=2 -o {output}.exe {file}"
    }
}